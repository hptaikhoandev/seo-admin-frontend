{
  "name": "data-spam-frontend",
  "version": "1.1.0",
  "private": true,
  "author": "okebaybi",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "start": "http-server ./dist -p 443 -S -C ./ssl/certificate.crt -K ./ssl/private.key",
    "build": "vue-tsc --noEmit && vite build",
    "build-stage": "vue-tsc --noEmit && vite build --base=/vue/free/stage/",
    "build-prod": "vue-tsc --noEmit && vite build --base=/vue/free/",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern '*.config.*' --format table",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --ignore-pattern '*.config.*' --format table"
  },
  "dependencies": {
    "@ant-design/icons-vue": "7.0.1",
    "@fontsource/inter": "5.0.18",
    "@fontsource/poppins": "5.0.14",
    "@fontsource/public-sans": "5.0.18",
    "@fontsource/roboto": "5.0.13",
    "@mdi/js": "^7.4.47",
    "@tiptap/starter-kit": "2.3.2",
    "@tiptap/vue-3": "2.3.2",
    "@tsconfig/node20": "20.1.4",
    "ant-design-vue": "4.2.1",
    "apexcharts": "3.49.1",
    "axios": "^1.7.3",
    "http-server": "^14.1.1",
    "jwt-decode": "^4.0.0",
    "pinia": "2.1.7",
    "vee-validate": "4.12.8",
    "vite-plugin-vuetify": "2.0.3",
    "vue": "3.4.27",
    "vue-api-query": "^1.11.0",
    "vue-chartjs": "^5.3.1",
    "vue-i18n": "9.13.1",
    "vue-router": "^4.4.0",
    "vue-tabler-icons": "2.21.0",
    "vue3-apexcharts": "1.5.2",
    "vue3-perfect-scrollbar": "2.0.0",
    "vuetify": "^3.6.10",
    "vuex": "^4.1.0",
    "webpack-plugin-vuetify": "3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@eslint/js": "^9.1.1",
    "@types/node": "20.12.11",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/tsconfig": "0.5.1",
    "eslint": "8.57.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-vue": "9.25.0",
    "prettier": "3.2.5",
    "sass": "1.77.1",
    "sass-loader": "14.2.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.8.0",
    "vite": "^5.2.11",
    "vue-cli-plugin-vuetify": "2.5.8",
    "vue-tsc": "2.0.17"
  }
}
